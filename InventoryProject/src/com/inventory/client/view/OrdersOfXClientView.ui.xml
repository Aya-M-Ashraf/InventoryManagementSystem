<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
		font-weight: bold;
		}
		.head
		{
		margin-left: 300px;
		align : center ;
		color : blue ;
		font-weight: bold;
		font-size: 20px;

		}
	</ui:style>
	<g:HTMLPanel>
		<div id="imageDiv1" />
		<g:Label ui:field="clientName" addStyleNames="{style.head}" />
		<g:Label ui:field="label" addStyleNames="{style.head}" />
		<div id="grid" />

	</g:HTMLPanel>
</ui:UiBinder> 